{
  "name": "streamview",
  "version": "1.0.0",
  "description": "RTSP Livestream with Overlay Management",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "setup-db": "node setup-database.js",
    "setup": "npm install && npm run setup-db"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "mongodb": "^6.20.0",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "rtsp",
    "livestream",
    "overlay",
    "video",
    "streaming"
  ],
  "author": "StreamView Team",
  "license": "MIT"
}
